{
  "name": "raneto",
  "version": "0.6.0",
  "description": "Markdown powered Knowledgebase",
  "author": "Gilbert Pellegrom <gilbert@pellegrom.me>",
  "license": "MIT",
  "keywords": [
    "raneto",
    "knowledgebase",
    "markdown"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gilbitron/Raneto.git"
  },
  "bugs": "https://github.com/gilbitron/Raneto/issues",
  "contributors": "https://github.com/gilbitron/Raneto/graphs/contributors",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://raw.githubusercontent.com/gilbitron/Raneto/master/LICENSE"
    }
  ],
  "main": "./app",
  "config": {
    "blanket": {
      "pattern": [
        "server"
      ],
      "data-cover-never": [
        "node_modules",
        "tests"
      ]
    }
  },
  "scripts": {
    "test": "mocha test/*.js --require should",
    "start": "node ./bin/www",
    "postinstall": "npm update"
  },
  "engines": {
    "node": "~0.10.0"
  },
  "dependencies": {
    "async": "^1.4.2",
    "bcrypt": "*",
    "body-parser": "~1.0.0",
    "connect-mongo": "*",
    "cookie-parser": "~1.0.1",
    "debug": "~0.7.4",
    "eventproxy": "^0.3.4",
    "express": "~4.2.0",
    "express-session": "^1.11.3",
    "extend": "^1.2.1",
    "glob": "^4.0.0",
    "hogan-express": "^0.5.2",
    "log4js": "^0.6.26",
    "lunr": "https://github.com/shunshunedu/lunr.js.git",
    "marked": "^0.3.2",
    "moment": "^2.6.0",
    "mongoose": "^4.1.6",
    "morgan": "~1.0.0",
    "nodemailer": "^1.4.0",
    "passport": "^0.3.0",
    "passport-local": "^1.0.0",
    "raneto-core": "0.x",
    "static-favicon": "~1.0.0",
    "underscore": "^1.6.0",
    "underscore.string": "^2.3.3",
    "validator": "^3.13.0"
  },
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-concurrent": "^2.0.3",
    "grunt-contrib-concat": "^0.5.1",
    "grunt-contrib-csslint": "^0.5.0",
    "grunt-contrib-cssmin": "^0.14.0",
    "grunt-contrib-jshint": "^0.11.3",
    "grunt-contrib-uglify": "^0.9.2",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-nodemon": "^0.4.0",
    "mm": "^1.3.1",
    "mocha": "^2.3.2",
    "pedding": "^1.0.0",
    "should": "^7.1.0",
    "supertest": "^1.1.0"
  }
}
